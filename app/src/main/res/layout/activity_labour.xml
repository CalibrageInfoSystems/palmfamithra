                    android:layout_toRightOf="@+id/dot2"
                    android:hint=""
                    android:text=""
                    android:textColor="@color/white"
                    android:textSize="14dp" />
                <TextView
                    android:fontFamily="@font/hind_semibold"
                    android:id="@+id/pru_d_amount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10sp"
                    android:layout_marginTop="1sp"
                    android:layout_toRightOf="@+id/pruning_amount"
                    android:hint=""
                    android:text=""
                    android:visibility="gone"
                    android:textColor="@color/white"
                    android:textSize="14dp" />

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/un_amount_label"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lin_one"
                android:layout_margin="6dp"
                android:visibility="gone"
                android:orientation="horizontal"

                >


                <TextView
                    android:id="@+id/un_amount_Label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="1sp"
                    android:fontFamily="@font/hind_semibold"
                    android:text="@string/intercrop_prunning"
                    android:textColor="@color/white"
                    android:textSize="14dp"
                    android:textStyle="bold" />


                <TextView
                    android:id="@+id/dotu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="10dp"
                    android:fontFamily="@font/hind_semibold"
                    android:text="                                    :"
                    android:textColor="@color/white"
                    android:textSize="12dp"
                    android:textStyle="bold" />

                <TextView
                    android:fontFamily="@font/hind_semibold"
                    android:id="@+id/un_amount"
                    android:layout_width="wrap_content"
