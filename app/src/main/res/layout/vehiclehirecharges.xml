<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/vehiclename"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="6dp"
        android:layout_marginTop="@dimen/margin_15"
        android:layout_weight="2"
        android:drawablePadding="5dp"
        android:fontFamily="@font/hind_semibold"
        android:text="@string/village"
        android:textColor="@color/blacknew"
        android:textSize="@dimen/album_title" />
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_height="wrap_content">



        <LinearLayout
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:weightSum="10"
            android:layout_height="wrap_content">



            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_weight="5"
                android:weightSum="10"
                android:layout_height="0dp">



                <LinearLayout

                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:weightSum="10"
                    android:layout_weight="5"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="6dp"
                        android:drawableRight="@drawable/star"
                        android:drawablePadding="5dp"
                        android:fontFamily="@font/hind_semibold"
                        android:paddingTop="5dp"
                        android:text="@string/destination1"
                        android:textColor="@color/blacknew"
                        android:textSize="@dimen/album_title" />


                    <RelativeLayout

                        android:layout_height="40dp"
                        android:layout_weight="5"
                        android:layout_margin="6dp"

                        android:layout_width="wrap_content"
                        android:background="@drawable/spinnerr_bg"
                        android:elevation="2dp">

                        <Spinner
                            android:id="@+id/destination1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/margin_10"
                            android:background="@null"
                            android:minHeight="0dp" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentBottom="true"
                            android:padding="@dimen/_8dp"
                            android:src="@drawable/down_arrow24" />
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout

                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:weightSum="10"
                    android:layout_weight="2"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:drawableRight="@drawable/star"
                        android:drawablePadding="5dp"
                        android:fontFamily="@font/hind_semibold"
                        android:paddingTop="5dp"
                        android:text="@string/price"
                        android:textColor="@color/blacknew"
                        android:textSize="@dimen/album_title" />
                    <EditText
                        android:id="@+id/price"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_weight="3"
                        android:layout_margin="@dimen/margin_5"
                        android:background="@drawable/edtt_bg"
                        android:cursorVisible="true"

                        android:fontFamily="@font/hind_semibold"
                        android:hint="@string/Rs"
                        android:inputType="numberDecimal"
                        android:maxLength="16"
                        android:paddingLeft="15dp"
                        android:singleLine="false"
                        android:textColor="@color/blacknew"
                        android:textColorHint="@color/half_black"
                        android:textSize="@dimen/album_title">

                    </EditText>
                </LinearLayout>
                <LinearLayout

                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:weightSum="10"
                    android:layout_weight="3"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/hiringbasics"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="6dp"
                        android:drawableRight="@drawable/star"
                        android:drawablePadding="5dp"
                        android:fontFamily="@font/hind_semibold"
                        android:paddingTop="5dp"
                        android:text="@string/hiringbasics"
                        android:textColor="@color/blacknew"
                        android:textSize="@dimen/album_title" />


                    <RelativeLayout

                        android:layout_height="40dp"
                        android:layout_weight="5"
                        android:layout_margin="6dp"

                        android:layout_width="wrap_content"
                        android:background="@drawable/spinnerr_bg"
                        android:elevation="2dp">

                        <Spinner
                            android:id="@+id/hiring_basics"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/margin_10"
                            android:background="@null"
                            android:minHeight="0dp" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentBottom="true"
                            android:padding="@dimen/_8dp"
                            android:src="@drawable/down_arrow24" />
                    </RelativeLayout>


                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_weight="5"
                android:weightSum="10"
                android:layout_height="0dp">



                <LinearLayout

                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:weightSum="10"
                    android:layout_weight="5"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/textView22"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="6dp"

                        android:drawablePadding="5dp"
                        android:fontFamily="@font/hind_semibold"
                        android:paddingTop="5dp"
                        android:text="@string/destination2"
                        android:textColor="@color/blacknew"
                        android:textSize="@dimen/album_title" />


                    <RelativeLayout

                        android:layout_height="40dp"
                        android:layout_weight="5"
                        android:layout_margin="6dp"

                        android:layout_width="wrap_content"
                        android:background="@drawable/spinnerr_bg"
                        android:elevation="2dp">

                        <Spinner
                            android:id="@+id/destination2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/margin_10"
                            android:background="@null"
                            android:minHeight="0dp" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentBottom="true"
                            android:padding="@dimen/_8dp"
                            android:src="@drawable/down_arrow24" />
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout

                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:weightSum="10"
                    android:layout_weight="2"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/textView32"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="6dp"

                        android:drawablePadding="5dp"
                        android:fontFamily="@font/hind_semibold"
                        android:paddingTop="5dp"
                        android:text="@string/price"
                        android:textColor="@color/blacknew"
                        android:textSize="@dimen/album_title" />

                    <EditText
                        android:id="@+id/price2"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_margin="@dimen/margin_5"
                        android:layout_weight="3"
                        android:background="@drawable/edtt_bg"
                        android:cursorVisible="true"
                        android:fontFamily="@font/hind_semibold"
                        android:hint="@string/Rs"
                        android:inputType="numberDecimal"
                        android:maxLength="16"
                        android:paddingLeft="15dp"
                        android:singleLine="false"
                        android:textColor="@color/blacknew"
                        android:textColorHint="@color/half_black"
                        android:textSize="@dimen/album_title">

                    </EditText>
                </LinearLayout>
                <LinearLayout

                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:weightSum="10"
                    android:layout_weight="3"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/hiringbasics2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="6dp"

                        android:drawablePadding="5dp"
                        android:fontFamily="@font/hind_semibold"
                        android:paddingTop="5dp"
                        android:text="@string/hiringbasics"
                        android:textColor="@color/blacknew"
                        android:textSize="@dimen/album_title" />


                    <RelativeLayout

                        android:layout_height="40dp"
                        android:layout_weight="5"
                        android:layout_margin="6dp"

                        android:layout_width="wrap_content"
                        android:background="@drawable/spinnerr_bg"
                        android:elevation="2dp">

                        <Spinner
                            android:id="@+id/hiring_basics2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/margin_10"
                            android:background="@null"
                            android:minHeight="0dp" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentBottom="true"
                            android:padding="@dimen/_8dp"
                            android:src="@drawable/down_arrow24" />
                    </RelativeLayout>


                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0.3dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/devider_black"></LinearLayout>
</LinearLayout>